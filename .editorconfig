root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
tab_width = 4

[*.gradle]
indent_style = space
indent_size = 4

[*.java]
indent_style = space
indent_size = 4
ij_java_imports_layout = $*,|,*
ij_java_class_count_to_use_import_on_demand = 999

[*.json]
indent_style = space
indent_size = 4

[*.properties]
indent_style = space
indent_size = 4

[.editorconfig]
indent_style = space
indent_size = 4

